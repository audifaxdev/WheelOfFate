{
  "name": "typescript-webpack-tree-shaking",
  "version": "1.0.0",
  "description": "Tree-shaking example with Typescript and Webpack",
  "main": "index.js",
  "scripts": {
    "webpack": "webpack --config webpack.config.js",
    "webpack-prod": "webpack --config webpack.prod.config.js",
    "webpack-es2015": "webpack --config webpack.es2015.config.js",
    "webpack-es2015-prod": "npm run webpack-es2015 && npm run babili",
    "babili": "concurrently \"babili dist/app.es2015.bundle.js --out-file dist/app.es2015.prod.bundle.js --no-comments\" \"babili dist/vendor.bundle.js --out-file dist/vendor.prod.bundle.js --no-comments\"",
    "serve": "http-server -a 0.0.0.0 -p 9000",
    "start": "npm run webpack-es2015-prod && npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blacksonic/typescript-webpack-tree-shaking.git"
  },
  "keywords": [
    "tree-shaking",
    "typescript",
    "webpack"
  ],
  "author": {
    "name": "AudifaxDev",
    "email": "audifaxdev@gmail.com"
  },
  "license": "ISC",
  "dependencies": {
    "@types/dat-gui": "^0.6.1",
    "@types/gsap": "^1.19.0",
    "@types/lodash": "^4.14.43",
    "@types/node": "^6.0.52",
    "@types/three": "0.0.28",
    "cannon": "^0.6.2",
    "gsap": "^1.19.0",
    "three": "^0.80.1",
    "three-orbit-controls": "^82.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-preset-latest": "^6.14.0",
    "babili": "^0.0.3",
    "concurrently": "^3.1.0",
    "http-server": "^0.9.0",
    "ts-loader": "^0.8.2",
    "typescript": "^2.0.0-dev",
    "typings": "^1.3.2",
    "webpack": "^2.1.0-beta.21"
  }
}
